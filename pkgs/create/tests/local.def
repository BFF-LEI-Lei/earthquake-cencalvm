PROJ_TMPDIR = $(BLD_TMPDIR)/$(PROJECT)/$(PACKAGE)/tests
PROJ_CLEAN += $(PROJ_BIN)

PROJ_CXX_INCLUDES += $(BLD_INCDIR)
PROJ_LCXX_FLAGS += -L$(BLD_LIBDIR)

# CPPUNIT
PROJ_CXX_INCLUDES += ${CPPUNIT_INCDIR}
ifdef CPPUNIT_LIBDIR
PROJ_LCXX_FLAGS += -L$(CPPUNIT_LIBDIR)
endif
PROJ_LCXX_FLAGS += -lcppunit -ldl

# ETREE
PROJ_CXX_INCLUDES += $(ETREE_INCDIR)
ifdef ETREE_LIBDIR
PROJ_LCXX_FLAGS += -L$(ETREE_LIBDIR)
endif
PROJ_LCXX_FLAGS += -letree

PROJ_LCXX_FLAGS += -lcencalvmstorage -lcencalvmcreate

