define MM_RECURSE
cd $(1); $(MM) $(BLD_ACTION); cd ..;
endef

recurse:
ifneq ($(RECURSE_DIRS),)
	$(foreach dir,$(RECURSE_DIRS),$(call MM_RECURSE,$(dir)))
endif

$(PROJ_SAR): product_dirs $(PROJ_OBJS)
	$(CXX) $(LCXX_SARFLAGS) -o $@ $(PROJ_OBJS) $(LCXXFLAGS)


.SECONDARY: $(PROJ_OBJS)

-include $(PROJ_DEPENDENCIES)

